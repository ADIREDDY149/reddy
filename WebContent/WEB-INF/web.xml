  <filter-class>com.slokam.FamilyServelet.TestFilter</filter-class>
  </filter>
  
  <filter-mapping>
  <filter-name>testfilter</filter-name>
  
  <url-pattern>/*</url-pattern>
  
  
  </filter-mapping>
  
</web-app>