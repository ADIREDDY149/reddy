  </servlet>
  
  <servlet-mapping>
  <servlet-name>save</servlet-name>
  <url-pattern>/savedata</url-pattern>
  
  </servlet-mapping>
  <servlet>
  <servlet-name>get</servlet-name>
  <servlet-class>com.slokam.FamilyServelet.GetAllServelet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>get</servlet-name>
  <url-pattern>/getall</url-pattern>
  
  </servlet-mapping>
  <servlet>
